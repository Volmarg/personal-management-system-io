<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>

        {{ encore_entry_link_tags('app') }}
        {{ encore_entry_link_tags('bootstrap') }}
        {% block stylesheets %}

        {% endblock %}
        {{ encore_entry_link_tags('adminkit') }} {# must always be last#}

    </head>
    {# See: https://demo.adminkit.io/ #}
    <body data-theme="light">
        <section id="vueApp"></section>
    </body>

    <footer>
        {{ encore_entry_script_tags('app') }}
        {{ encore_entry_script_tags('bootstrap') }}
        {% block javascripts %}

        {% endblock %}
        {{ encore_entry_script_tags('adminkit') }} {# must always be last#}
    </footer>
</html>
